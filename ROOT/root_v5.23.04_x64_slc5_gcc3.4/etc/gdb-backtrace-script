# This script is used by TUnixSystem::StackTrace() on Mac OS X.

echo \n
echo ========== STACKS OF ALL THREADS ==========\n
thread apply all bt

#echo \n
#echo ========== LOCAL VARIABLE INFORMATION ==========\n
#info locals

#echo \n
#echo ========== REGISTER VALUES ==========\n
#info all-registers

#echo \n
#echo ========== SHARED LIBRARY ADDRESS INFORMATION ==========\n
#info sharedlibrary

detach
quit
